{% extends 'base.html.twig' %}

{% block title %}Clearance{% endblock %}

{% block body %}
    <h1>Clearance</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ clearance.id }}</td>
            </tr>
            <tr>
                <th>Completed</th>
                <td>{{ clearance.completed ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ clearance.createdAt ? clearance.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ clearance.updatedAt ? clearance.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_clearance_index') }}">back to list</a>

    <a href="{{ path('app_clearance_edit', {'id': clearance.id}) }}">edit</a>

    {{ include('clearance/_delete_form.html.twig') }}
{% endblock %}
